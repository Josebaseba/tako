#circular3dG
  position relative
  height LOADER_SIZE

.circular3dG
  position absolute
  background-color #000000
  width LOADER_BALL
  height LOADER_BALL
  border-radius 3.2px
  animation-name(bounce_circular3dG)
  animation-duration(INIT_DELAY + DELAY_SUM * 7)
  animation-iteration-count(infinite)
  animation-direction(linear)

#circular3d_1G
  left 33px
  top 4px
  animation-delay(INIT_DELAY)

#circular3d_2G
  left 46px
  top 15px
  animation-delay(INIT_DELAY + DELAY_SUM)

#circular3d_3G
  left 54px
  top 29px
  animation-delay(INIT_DELAY + DELAY_SUM * 2)

#circular3d_4G
  left 51px
  top 43px
  animation-delay(INIT_DELAY + DELAY_SUM * 3)

#circular3d_5G
  left 34px
  top 47px
  animation-delay(INIT_DELAY + DELAY_SUM * 4)

#circular3d_6G
  left 12px
  top 31px
  animation-delay(INIT_DELAY + DELAY_SUM * 5)

#circular3d_7G
  left 7px
  top 9px
  animation-delay(INIT_DELAY + DELAY_SUM * 6)

#circular3d_8G
  left 18px
  top 0px
  animation-delay(INIT_DELAY + DELAY_SUM * 7)

@keyframes bounce_circular3dG
    0%
      transform scale(2.6)
    100%
      transform scale(1)

